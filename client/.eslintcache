firm(\r\n        'Вы уверены, что хотите удалить этот документ?'\r\n      );\r\n\r\n      if (answer) {\r\n        await axios.delete(`/api/remove_document/${documentId}`, {\r\n          headers: { Authorization: token },\r\n        });\r\n        history.push('/data_base');\r\n      }\r\n    } catch (err) {\r\n      console.log(err);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"document-card\">\r\n      {filterFaculty.map((item) => (\r\n        <Card\r\n          key={item._id}\r\n          hoverable\r\n          cover={<img alt=\"Document image\" src={documentImage} />}\r\n        >\r\n          <div className=\"description\">\r\n            <h3>Status:</h3> {item.step === 4 ? 'Completed' : 'In progress'}\r\n            <br />\r\n            <h3>Name:</h3> {item.name}\r\n            <br />\r\n            <br />\r\n            <p>\r\n              <strong>Number:</strong> <br />\r\n              {item._id}\r\n            </p>\r\n            <p>\r\n              <strong>Loaded:</strong>\r\n              <br />\r\n              {moment(item.createdAt).format('DD.MM.YYYY, hh:mm:ss')}\r\n            </p>\r\n            <p>\r\n              <strong>Last modified:</strong>\r\n              <br />\r\n              {moment(item.lastModified).format('DD.MM.YYYY, hh:mm:ss')}\r\n            </p>\r\n            <p>\r\n              <strong>Format:</strong> <br />.{item.type}\r\n            </p>\r\n            <p>\r\n              <strong>Size:</strong> <br />{' '}\r\n              {Math.ceil((item.size / 1024) * 100) / 100} Кб\r\n            </p>\r\n          </div>\r\n\r\n          <div className=\"buttons\">\r\n            <Link\r\n              to={`/documents/${item.name}`}\r\n              // to={`/documents/${item.name}.${item.type}`}\r\n              target=\"_blank\"\r\n              download\r\n            >\r\n              <Button type=\"primary\" icon={<DownloadOutlined />}>\r\n                Download\r\n              </Button>\r\n            </Link>